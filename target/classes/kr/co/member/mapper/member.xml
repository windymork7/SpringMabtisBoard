<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="kr.co.DAO.MemberIDao">

	<insert id="JoinProcess" parameterType="kr.co.DTO.MemberDTO">
		INSERT INTO MEMBERINFORMATION VALUES
		<foreach collection="item" item="item" separator=",">
			(#{item})
		</foreach>
		<!-- (#{param1}, #{param2}, #{param3}, #{param4}, #{param5}, #{param6}, #{param7}, #{param8}, #{param9}, #{param10}, #{param11}, #{param12}) -->
	</insert>
	

</mapper>
    
